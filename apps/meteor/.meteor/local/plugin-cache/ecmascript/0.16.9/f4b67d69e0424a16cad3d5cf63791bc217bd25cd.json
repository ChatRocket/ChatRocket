ded":true,"_verified":true},"options":{},"externalDependencies":[]}],"presets":[],"generatorOpts":{"filename":"/Users/guilhermegazzo/dev/Rocket.Chat/apps/meteor/packages/rocketchat:restivus/lib/iron-router-error-to-response.js","comments":true,"compact":false,"sourceMaps":true,"sourceFileName":"packages/rocketchat:restivus/lib/iron-router-error-to-response.js"}},"code":"module.export({\n  ironRouterSendErrorToResponse: () => ironRouterSendErrorToResponse\n});\n// We need a function that treats thrown errors exactly like Iron Router would.\n// This file is written in JavaScript to enable copy-pasting Iron Router code.\n\n// Taken from: https://github.com/iron-meteor/iron-router/blob/9c369499c98af9fd12ef9e68338dee3b1b1276aa/lib/router_server.js#L3\nconst env = process.env.NODE_ENV || 'development';\n\n// Taken from: https://github.com/iron-meteor/iron-router/blob/9c369499c98af9fd12ef9e68338dee3b1b1276aa/lib/router_server.js#L47\nfunction ironRouterSendErrorToResponse(err, req, res) {\n  if (res.statusCode < 400) res.statusCode = 500;\n  if (err.status) res.statusCode = err.status;\n  let msg;\n  if (env === 'development') msg = \"\".concat(err.stack || err.toString(), \"\\n\");\n  // XXX get this from standard dict of error messages?\n  else msg = 'Server error.';\n  console.error(err.stack || err.toString());\n  if (res.headersSent) return req.socket.destroy();\n  res.setHeader('Content-Type', 'text/html');\n  res.setHeader('Content-Length', Buffer.byteLength(msg));\n  if (req.method === 'HEAD') return res.end();\n  res.end(msg);\n}","map":{"version":3,"names":["module","export","ironRouterSendErrorToResponse","env","process","NODE_ENV","err","req","res","statusCode","status","msg","concat","stack","toString","console","error","headersSent","socket","destroy","setHeader","Buffer","byteLength","method","end"],"sources":["packages/rocketchat:restivus/lib/iron-router-error-to-response.js"],"sourcesContent":["// We need a function that treats thrown errors exactly like Iron Router would.\n// This file is written in JavaScript to enable copy-pasting Iron Router code.\n\n// Taken from: https://github.com/iron-meteor/iron-router/blob/9c369499c98af9fd12ef9e68338dee3b1b1276aa/lib/router_server.js#L3\nconst env = process.env.NODE_ENV || 'development';\n\n// Taken from: https://github.com/iron-meteor/iron-router/blob/9c369499c98af9fd12ef9e68338dee3b1b1276aa/lib/router_server.js#L47\nexport function ironRouterSendErrorToResponse(err, req, res) {\n\tif (res.statusCode < 400) res.statusCode = 500;\n\n\tif (err.status) res.statusCode = err.status;\n\n\tlet msg;\n\tif (env === 'development') msg = `${err.stack || err.toString()}\\n`;\n\t// XXX get this from standard dict of error messages?\n\telse msg = 'Server error.';\n\n\tconsole.error(err.stack || err.toString());\n\n\tif (res.headersSent) return req.socket.destroy();\n\n\tres.setHeader('Content-Type', 'text/html');\n\tres.setHeader('Content-Length', Buffer.byteLength(msg));\n\tif (req.method === 'HEAD') return res.end();\n\tres.end(msg);\n}\n"],"mappings":"AAAAA,MAAM,CAACC,MAAM,CAAC;EAACC,6BAA6B,EAACA,CAAA,KAAIA;AAA6B,CAAC,CAAC;AAAhF;AACA;;AAEA;AACA,MAAMC,GAAG,GAAGC,OAAO,CAACD,GAAG,CAACE,QAAQ,IAAI,aAAa;;AAEjD;AACO,SAASH,6BAA6BA,CAACI,GAAG,EAAEC,GAAG,EAAEC,GAAG,EAAE;EAC5D,IAAIA,GAAG,CAACC,UAAU,GAAG,GAAG,EAAED,GAAG,CAACC,UAAU,GAAG,GAAG;EAE9C,IAAIH,GAAG,CAACI,MAAM,EAAEF,GAAG,CAACC,UAAU,GAAGH,GAAG,CAACI,MAAM;EAE3C,IAAIC,GAAG;EACP,IAAIR,GAAG,KAAK,aAAa,EAAEQ,GAAG,MAAAC,MAAA,CAAMN,GAAG,CAACO,KAAK,IAAIP,GAAG,CAACQ,QAAQ,CAAC,CAAC,OAAI;EACnE;EAAA,KACKH,GAAG,GAAG,eAAe;EAE1BI,OAAO,CAACC,KAAK,CAACV,GAAG,CAACO,KAAK,IAAIP,GAAG,CAACQ,QAAQ,CAAC,CAAC,CAAC;EAE1C,IAAIN,GAAG,CAACS,WAAW,EAAE,OAAOV,GAAG,CAACW,MAAM,CAACC,OAAO,CAAC,CAAC;EAEhDX,GAAG,CAACY,SAAS,CAAC,cAAc,EAAE,WAAW,CAAC;EAC1CZ,GAAG,CAACY,SAAS,CAAC,gBAAgB,EAAEC,MAAM,CAACC,UAAU,CAACX,GAAG,CAAC,CAAC;EACvD,IAAIJ,GAAG,CAACgB,MAAM,KAAK,MAAM,EAAE,OAAOf,GAAG,CAACgB,GAAG,CAAC,CAAC;EAC3ChB,GAAG,CAACgB,GAAG,CAACb,GAAG,CAAC;AACb","ignoreList":[]},"sourceType":"